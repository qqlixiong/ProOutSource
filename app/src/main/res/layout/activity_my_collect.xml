<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/activity_my_collect"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.lixiong.straight.my.collect.MyCollectActivity">

    <com.lixiong.straight.view.TopBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tv_title="我的收藏"/>

    <com.lixiong.straight.my.view.NoDataView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tv_no_data_a="您还没有任何收藏"
        app:tv_no_data_b="快去项目详情页中收藏项目吧"/>
</LinearLayout>

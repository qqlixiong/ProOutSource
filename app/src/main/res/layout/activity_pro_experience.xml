<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/activity_pro_experience"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lixiong.straight.my.experience.activity.ProExperienceActivity">

    <FrameLayout
        android:id="@+id/fl_pro_experience_head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <FrameLayout
        android:id="@+id/fl_pro_experience_content"
        android:layout_below="@id/fl_pro_experience_head"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>

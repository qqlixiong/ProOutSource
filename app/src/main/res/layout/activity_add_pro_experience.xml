<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/activity_add_pro_experience"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lixiong.straight.my.experience.activity.AddProExperienceActivity">

    <com.lixiong.straight.view.TopBar
        android:id="@+id/top_add_pro_experience"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tv_title="@string/pro_experience"/>

    <FrameLayout
        android:id="@+id/fl_add_pro_experience"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/top_add_pro_experience"
        android:background="@color/back_ground_color"/>
</RelativeLayout>
